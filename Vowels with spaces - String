import java.io.;
import java.util.;
import java.text.;
import java.math.;
import java.util.regex.*;

public class Solution {

public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    String s1=sc.nextLine();
    for(int i=0;i<s1.length();i++)
    {
        char c=s1.charAt(i);
        if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u')
        System.out.print(" ");
        else
            System.out.print(c);
    }
}
}
