import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class sollution{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
if(n<100)
System.out.println("Invalid Input");
else{
int sum=0;
while(n>0){
sum+=n%10;
n/=10;
}
System.out.println("Sum of digit is "+sum);
}
}
}
